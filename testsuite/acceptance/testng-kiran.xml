<?xml version="1.0" encoding="UTF-8"?>
<suite name="RulesEngine UI Automation Project" preserve-order="true">
    <listeners>
        <listener class-name="com.gainsight.utils.AutTestListener"/>
        <listener class-name="com.gainsight.utils.testlink.TestLinkListener"/>
    </listeners>
    <test verbose="3" name="RulesEngine UI Automation TestCases" preserve-order="true">
        <classes>
            <class name="com.gainsight.bigdata.rulesengine.tests.CallToActionWithRelationshipsTest">
                <methods>
                    <exclude name="createCtaUsingMdaLinkedCollection"/>
                </methods>
            </class>
            <class name="com.gainsight.bigdata.rulesengine.tests.CreateRuleTest"/>
            <class name="com.gainsight.bigdata.rulesengine.tests.CallToActionTest"/>
            <class name="com.gainsight.bigdata.rulesengine.tests.LoadToMilestonesAndFeaturesTest"/>
            <class name="com.gainsight.bigdata.rulesengine.tests.LoadToCustomersTest"/>
            <class name="com.gainsight.bigdata.rulesengine.tests.LoadToUsageTest"/>
            <class name="com.gainsight.bigdata.rulesengine.tests.SetScoreActionWithNumericSchemeTest"/>
            <class name="com.gainsight.bigdata.rulesengine.tests.SetScoreActionWithGradingSchemeTest"/>
            <class name="com.gainsight.bigdata.rulesengine.tests.SetScoreActionWithColorSchemeTest"/>
            <class name="com.gainsight.bigdata.rulesengine.tests.CalculatedFieldsAndMeasuresTest"/>
            <class name="com.gainsight.bigdata.rulesengine.tests.SetupRuleActionsWithNativeDataTest"/>
        </classes>
        <classes>
            <parameter name="dbStoreType" value="Mongo"/>
            <class name="com.gainsight.bigdata.rulesengine.tests.LoadToMdaTest"/>
            <class name="com.gainsight.bigdata.rulesengine.tests.LoadToCustomersTestUsingMongoAsSourceData"/>
            <class name="com.gainsight.bigdata.rulesengine.tests.LoadToCustomersTestUsingMatrixData"/>
            <class name="com.gainsight.bigdata.rulesengine.tests.CalculatedFieldsAndMeasuresTestUsingMatrixData"/>
            <class name="com.gainsight.bigdata.rulesengine.tests.CalculatedFieldsAndMeasuresTestUsingMongoAsSourceData"/>
            <class name="com.gainsight.bigdata.rulesengine.tests.CallToActionWithRelationshipsTest">
                <methods>
                    <include name="createCtaUsingMdaLinkedCollection"/>
                </methods>
            </class>
        </classes>
        <classes>
            <parameter name="dbStoreType" value="Redshift"/>
            <class name="com.gainsight.bigdata.rulesengine.tests.LoadToMdaTest"/>
            <class name="com.gainsight.bigdata.rulesengine.tests.LoadToSfdcTest"/>
            <class name="com.gainsight.bigdata.rulesengine.tests.LoadToCustomersTestUsingMatrixData"/>
            <class name="com.gainsight.bigdata.rulesengine.tests.LoadToCustomersTestUsingRedshiftAsSourceData"/>
            <class name="com.gainsight.bigdata.rulesengine.tests.CalculatedFieldsAndMeasuresTestUsingMatrixData"/>
            <class name="com.gainsight.bigdata.rulesengine.tests.CalculatedFieldsAndMeasuresTestUsingRedShiftAsSourceData"/>
        </classes>
        <classes>
            <parameter name="dbStoreType" value="POSTGRES"/>
            <class name="com.gainsight.bigdata.rulesengine.tests.CalculatedFieldsAndMeasuresTestUsingMatrixData"/>
            <class name="com.gainsight.bigdata.rulesengine.tests.LoadToCustomersTestUsingMatrixData"/>
        </classes>
    </test>
</suite>