<?xml version="1.0" encoding="UTF-8"?>
<suite name="RulesEngine UI Automation Project" preserve-order="true">
   <listeners>
      <listener class-name="com.gainsight.utils.AutTestListener" />
      <listener class-name="com.gainsight.utils.testlink.TestLinkListener" />
   </listeners>
      <test verbose="3" name="RulesEngine UI Automation TestCases Using MDA MONGO DataBase" preserve-order="true">
      <parameter name="dbStoreType" value="Mongo" />
      <classes>
         <class name="com.gainsight.bigdata.rulesengine.tests.CreateRuleTest">
            <methods>
               <include name="testCtaActionWithCalculatedMeasuresUsingMdaSubjectArea" />
               <include name="testCtaActionWithCalculatedFieldsAndMeasuresUsingMdaSubjectArea" />
               <include name="testAllActionsUsingMdaData" />
               <include name="testLoadToCustomersWithMatrixData" />
               <include name="loadToMdaActionUsingNativeData" />
               <include name="verifyRecordsOnDateTimeFiltersUsingMdaData" />
               <include name="verifyRecordsOnDateFiltersUsingMdaData" />
            </methods>
         </class>
      </classes>
   </test>
   
   <test verbose="3" name="RulesEngine UI Automation Tests Using Native Data" preserve-order="true">
      <classes>
         <class name="com.gainsight.bigdata.rulesengine.tests.CreateRuleTest">
            <methods>
               <include name="testRuleInactive" />
               <include name="testCloningOFARule" />
               <include name="testDeleteRule" />
               <include name="testEditARule" />
               <include name="testCtaWithUpsertProrityOption" />
               <include name="testCtaWithUpdateCommentsAlwaysOption" />
               <include name="testCtaWithAddOrReplacePlaybook" />
               <include name="testCtaWithRuleNameChangeOption" />
               <include name="testCtaUpsertWithSnoozeOption" />
               <include name="testCloseCtaFromSpecificSource" />
               <include name="testCloseCtaFromAllSources" />
               <include name="testCTAActionWithCalculatedFieldsOnUsageDataObject" />
            </methods>
         </class>
      </classes>
   </test>
</suite>