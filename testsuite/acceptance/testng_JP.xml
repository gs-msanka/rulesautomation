<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<suite name="Connectors Project" preserve-order="true">

    <listeners>
        <listener class-name="com.gainsight.utils.AutTestListener" />
        <listener class-name="com.gainsight.utils.testlink.TestLinkListener" />
    </listeners>



    <test verbose="3" name="Load To MDA MONGO" preserve-order="true">
        <parameter name="dbStoreType" value="MONGO"/>
        <classes>
            <class name="com.gainsight.sfdc.reporting.mda.setup.LoadMDAReports" />
        </classes>
    </test>

    <test verbose="3" name="Load To MDA REDSHIFT" preserve-order="true">
        <parameter name="dbStoreType" value="REDSHIFT"/>
        <classes>
            <class name="com.gainsight.sfdc.reporting.mda.setup.LoadMDAReports" />
        </classes>
    </test>

</suite>

