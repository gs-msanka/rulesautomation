<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<suite name="Copilot Project" preserve-order="true">

    <listeners>
        <listener class-name="com.gainsight.utils.AutTestListener"/>
        <listener class-name="com.gainsight.utils.testlink.TestLinkListener"/>
    </listeners>
    <test verbose="3" name="Copilot Application Test Suite" preserve-order="true">
        <classes>
            <class name="com.gainsight.bigdata.copilot.tests.SfdcPowerlistStrategies"/>
        </classes>
    </test>
    <test verbose="3" name="Copilot TestCases Using  Mongo DataBase" preserve-order="true">
        <parameter name="dbStoreType" value="Mongo"/>
        <classes>
            <class name="com.gainsight.bigdata.copilot.tests.CopilotWithMdaDataTest"/>
        </classes>
    </test>
    <test verbose="3" name="Copilot TestCases Using Redshift DataBase" preserve-order="true">
        <parameter name="dbStoreType" value="Redshift"/>
        <classes>
            <class name="com.gainsight.bigdata.copilot.tests.CopilotWithMdaDataTest">
                <methods>
                    <include name="testAccountStrategyWithMdaCollection"/>
                    <include name="testContactStrategyWithMdaCollection"/>
                    <include name="testEmailStrategyWithMdaCollection"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>
