{
  "jobName": "Connectors Account Upsert",
  "jobType": "AccountUpsert",
  "namespacePrefix": "JBCXM",
  "useNameSpace": false,
  "extract": {
    "source": "sfdc",
    "connection": "conf/application.conf",
    "table": "Account",
    "useRestApi"  : true,
    "fields": [
      "Id",
      "Name"
    ],
    "output": "/testdata/newstack/CoPilot/dataSet/process/Accounts.csv"
  },
  "transform": {
    "query": "",
    "useQuery": false,
    "tableInfo": [
      {
        "file": "/testdata/newstack/CoPilot/dataSet/process/Accounts.csv",
        "table": "TempAccount",
        "joinColumnName": "Name",
        "columns": [
          {
            "name": "Id",
            "alias": "Id"
          }
        ]
      },
      {
        "file": "/testdata/newstack/CoPilot/dataSet/data/CopilotAccounts.csv",
        "table": "ConnAccount",
        "joinColumnName": "Name",
        "columns": [
          {
            "name": "Name",
            "alias": "Name"
          },
          {
            "name": "Account_ExternalId__c",
            "alias": "Account_ExternalId__c"
          }
        ]
      }
    ],
    "join": true,
    "picklist": false,
    "output": "/testdata/newstack/CoPilot/dataSet/process/Final_Accounts.csv"
  },
  "load": {
    "target": "sfdc",
    "sObject": "Account",
    "operation": "upsert",
    "contentType": "CSV",
    "externalIDField": "Id",
    "file": "/testdata/newstack/CoPilot/dataSet/process/Final_Accounts.csv"
  }
}