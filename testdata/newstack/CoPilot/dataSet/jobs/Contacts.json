{
  "extract": {
    "source": "sfdc",
    "connection": "conf/application.conf",
    "table": "Account",
    "fields": [
      "Id",
      "Name",
      "Account_ExternalId__c"
    ],
    "output": "/testdata/newstack/CoPilot/dataSet/process/Final_Accounts.csv"
  },
  "transform": {
    "query" : "",
    "useQuery" : false,
    "tableInfo": [
      {
        "file": "/testdata/newstack/CoPilot/dataSet/process/Final_Accounts.csv",
        "table": "TempAccount",
        "joinColumnName": "Name",
        "columns": [
          {
            "name": "Id",
            "alias": "AccountId"
          }
        ]
      },
      {
        "file": "/testdata/newstack/CoPilot/dataSet/data/CopilotContacts.csv",
        "table": "ConnAccount",
        "joinColumnName": "AccountName",
        "columns": [
          {
            "name": "LastName",
            "alias": "LastName"
          },
          {
            "name": "FirstName",
            "alias": "FirstName"
          },
          {
            "name": "Email",
            "alias": "Email"
          },
          {
            "name": "Contact_ExternalId__c",
            "alias": "Contact_ExternalId__c"
          }
        ]
      }
    ],
    "join": true,
    "picklist": false,
    "output": "/testdata/newstack/CoPilot/dataSet/process/Final_Contacts.csv"
  },
  "load": {
    "target": "sfdc",
    "sObject": "Contact",
    "operation": "upsert",
    "contentType": "CSV",
    "externalIDField": "Contact_ExternalId__c",
    "file": "/testdata/newstack/CoPilot/dataSet/process/Final_Contacts.csv"
  }
}