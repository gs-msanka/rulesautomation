{
  "jobName": "Update Account names and ID",
  "jobType": "AccountUpsert",
  "namespacePrefix": "JBCXM",
  "useNameSpace": false,
  "extract": {
    "useRestApi": true,
    "source": "sfdc",
    "connection": "conf/application.conf",
    "table": "Account",
    "fields": [
      "Id",
      "Name"
    ],
    "output": "/resources/datagen/process/ReportingUIAccountsss.csv"
  },
  "transform": {
    "query": "",
    "useQuery": false,
    "tableInfo": [
      {
        "file": "/resources/datagen/process/ReportingUIAccountsss.csv",
        "table": "account",
        "joinColumnName": "Name",
        "columns": [
          {
            "name": "ID",
            "alias": "ID"
          },
          {
            "name": "Name",
            "alias": "AccountName"
          }
        ]
      },
      {
        "file": "/testdata/newstack/reporting/data/ReportDataRedShift.csv",
        "table": "accounts",
        "joinColumnName": "ProductName",
        "columns": [
          {
            "name": "ProductCode",
            "alias": "ProductCode"
          },
          {
            "name": "ProductName",
            "alias": "ProductName"
          },
          {
            "name": "Date",
            "alias": "Date"
          },
          {
            "name": "EventTimeStamp",
            "alias": "EventTimeStamp"
          },
          {
            "name": "Active",
            "alias": "Active"
          },
          {
            "name": "PageViews",
            "alias": "PageViews"
          },
          {
            "name": "PageVisits",
            "alias": "PageVisits"
          },
          {
            "name": "FilesDownloaded",
            "alias": "FilesDownloaded"
          },
          {
            "name": "NoofReportsRun",
            "alias": "NoofReportsRun"
          },
          {
            "name": "NoofRulesTriggered",
            "alias": "NoofRulesTriggered"
          },
          {
            "name": "NoofSchedulesCreated",
            "alias": "NoofSchedulesCreated"
          },
          {
            "name": "Industry",
            "alias": "Industry"
          }
        ]
      }
    ],
    "join": true,
    "picklist": false,
    "output": "/testdata/newstack/reporting/data/ReportData2.csv"
  }
}