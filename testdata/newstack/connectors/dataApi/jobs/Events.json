{
  "jobName": "EventProcess",
  "jobType": "EventProcess",
  "namespacePrefix": "JBCXM",
  "extract": {
    "source": "sfdc",
    "connection": "conf/application.conf",
    "table": "Contact",
    "fields": [
      "Id",
      "AccountId",
      "Contact_ExternalId__c",
      "Name",
      "LastName",
      "FirstName",
      "Email"
    ],
    "output": "/testdata/newstack/connectors/dataApi/process/Contacts.csv"
  },
  "dateProcess" : {
    "inputFile" : "/testdata/newstack/connectors/dataApi/data/Events.csv",
    "outputFile" : "/testdata/newstack/connectors/dataApi/process/Date_Processed_RawEventsData.csv",
    "fields" :[
      {
        "weekly" : false,
        "daily"  : true,
        "dateTime" : false,
        "dateFormat" : "MM-dd-yyyy",
        "fieldName" : "Date"
      },
      {
        "weekly" : false,
        "daily"  : true,
        "dateTime" : true,
        "dateFormat" : "MM-dd-yyyy HH:mm:ss",
        "fieldName" : "EventTimeStamp"
      }
    ]
  },
  "transform": {
    "query" : "",
    "useQuery" : false,
    "tableInfo": [
      {
        "file": "/testdata/newstack/connectors/dataApi/process/Contacts.csv",
        "table": "TempContactBridge",
        "joinColumnName": "Contact_ExternalId__c",
        "columns": [
          {
            "name": "Id",
            "alias": "GSA_ContactID"
          },
          {
            "name": "AccountId",
            "alias": "GSA_AccountID"
          },
          {
            "name": "LastName",
            "alias": "GSA_LastName"
          },
          {
            "name": "FirstName",
            "alias": "GSA_FirstName"
          },
          {
            "name": "Name",
            "alias": "GSA_ContactName"
          },
          {
            "name": "Email",
            "alias": "GSA_Email"
          }
        ]
      },
      {
        "file": "/testdata/newstack/connectors/dataApi/process/Date_Processed_RawEventsData.csv",
        "table": "RawEvents",
        "joinColumnName": "Contact_ExternalId__c",
        "columns": [
          {
            "name": "AccountName",
            "alias": "GSA_AccountName"
          },
          {
            "name": "Account_ExternalId__c",
            "alias": "GSA_Account_ExternalId__c"
          },
          {
            "name": "Contact_ExternalId__c",
            "alias": "GSA_Contact_ExternalId__c"
          },
          {
            "name": "Event",
            "alias": "GSA_Event"
          },
          {
            "name": "Date",
            "alias": "Date"
          },
          {
            "name": "EventTimeStamp",
            "alias": "EventTimeStamp"
          },
          {
            "name": "Logins",
            "alias": "Logins"
          },
          {
            "name": "UniqueLogins",
            "alias": "UniqueLogins"
          },
          {
            "name": "PageLoadTime",
            "alias": "PageLoadTime"
          },
          {
            "name": "FileSize",
            "alias": "FileSize"
          },
          {
            "name": "Price",
            "alias": "Price"
          }
        ]
      }
    ],
    "join": true,
    "picklist": false,
    "output": "/testdata/newstack/connectors/dataApi/process/Final_RawEventsData.csv"
  }
}