{
  "extract": {
    "source": "sfdc",
    "connection": "conf/application.conf",
    "table": "Account",
    "fields": [
      "Id",
      "Name",
      "Account_ExternalId__c"
    ],
    "whereCondition" : " where Name Like 'Account Agg-21%'",
    "output": "/testdata/newstack/connectors/dataApi/process/t21/Accounts.csv"
  },
  "transform": {
    "query" : "",
    "useQuery" : false,
    "tableInfo": [
      {
        "file": "/testdata/newstack/connectors/dataApi/process/t21/Accounts.csv",
        "table": "TempAccount",
        "joinColumnName": "Account_ExternalId__c",
        "columns": [
          {
            "name": "Id",
            "alias": "AccountId"
          }
        ]
      },
      {
        "file": "/testdata/newstack/connectors/dataApi/tests/t21/Contacts.csv",
        "table": "ConnAccount",
        "joinColumnName": "Account_ExternalId__c",
        "columns": [
          {
            "name": "LastName",
            "alias": "LastName"
          },
          {
            "name": "FirstName",
            "alias": "FirstName"
          },
          {
            "name": "Email",
            "alias": "Email"
          },
          {
            "name": "Contact_Ext_NU__c",
            "alias": "Contact_Ext_NU__c"
          }
        ]
      }
    ],
    "join": true,
    "picklist": false,
    "output": "/testdata/newstack/connectors/dataApi/process/t21/Final_Contacts.csv"
  },
  "load": {
    "target": "sfdc",
    "sObject": "Contact",
    "operation": "insert",
    "contentType": "CSV",
    "file": "/testdata/newstack/connectors/dataApi/process/t21/Final_Contacts.csv"
  }
}